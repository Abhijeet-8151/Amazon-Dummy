<?xml version="1.0" encoding="UTF-8"?>

<mule xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:munit="http://www.mulesoft.org/schema/mule/munit" xmlns:munit-tools="http://www.mulesoft.org/schema/mule/munit-tools"
	xmlns="http://www.mulesoft.org/schema/mule/core" xmlns:doc="http://www.mulesoft.org/schema/mule/documentation"
	xsi:schemaLocation="
		http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd
		http://www.mulesoft.org/schema/mule/munit http://www.mulesoft.org/schema/mule/munit/current/mule-munit.xsd
		http://www.mulesoft.org/schema/mule/munit-tools  http://www.mulesoft.org/schema/mule/munit-tools/current/mule-munit-tools.xsd">
	<munit:config name="wishlist-test-suite.xml" />
	<munit:test name="wishlist-test-suite-getWishlistTest" doc:id="b663906e-37e8-4569-a5fc-0fc65a2ba61d" description="Test">
		<munit:execution >
			<flow-ref doc:name="Flow-ref to getWishlist" doc:id="be3eb782-8afb-46c8-8e36-61a3ab749917" name="getWishlist"/>
		</munit:execution>
		<munit:validation >
			<munit-tools:assert-that doc:name="Assert that" doc:id="3b4f2aad-ddd0-4259-860e-51e67c5cf4f5" expression="#[payload]" is="#[MunitTools::notNullValue()]"/>
		</munit:validation>
	</munit:test>
	<munit:test name="wishlist-test-suite-postWishlistTest" doc:id="cdb6574e-e144-46ce-8c8b-93170f6c1f56" description="Test">
		<munit:execution >
			<munit:set-event doc:name="Set Event" doc:id="ece4b1ee-2d0b-4ccc-b290-cbb198091b9e" >
				<munit:payload value='#[output application/json&#10;---&#10;[{   "Price": "1000",&#10;	"Item_Name": "Water bottle",&#10;	"Item_ID": "5",&#10;	"Availability": "Y"}]]' />
				<munit:attributes value="#[{queryParams:{email:'test@munit.com'}}]" />
			</munit:set-event>
			<flow-ref doc:name="Flow-ref to postWishlist" doc:id="363b08ce-87b9-4f4d-9e3f-980bb2de8896" name="postWishlist"/>
		</munit:execution>
		<munit:validation >
			<munit-tools:assert-that doc:name="Assert that" doc:id="94305332-e215-4da3-a337-fc8f910dc475" is="#[MunitTools::notNullValue()]" expression="#[payload]" message="Failed"/>
		</munit:validation>
	</munit:test>


</mule>
